{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": "logs:CreateLogGroup",
			"Resource": "arn:aws:logs:eu-west-2:AWS_ACCOUNT_ID:*"
		},
		{
			"Effect": "Allow",
			"Action": ["logs:CreateLogStream", "logs:PutLogEvents"],
			"Resource": [
				"arn:aws:logs:eu-west-2:AWS_ACCOUNT_ID:log-group:/aws/lambda/FUNCTION_NAME:*"
			]
		}
	]
}
